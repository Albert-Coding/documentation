{
  "typescript": {
    "bracketSpacing": true,
    "singleQuote": true
  },
  "json": {
  },
  "markdown": {
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": false,
    "embeddedLanguageFormatting": "auto",
    "htmlWhitespaceSensitivity": "css",
    "insertPragma": false,
    "jsxBracketSameLine": false,
    "jsxSingleQuote": false,
    "printWidth": 80,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "requirePragma": false,
    "semi": true,
    "singleQuote": false,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "includes": ["**/*.{ts,tsx,js,jsx,cjs,mjs,json,md}"],
  "excludes": [
    "**/node_modules",
    "**/*-lock.json",
    "blogs",
    ".docusaurus/",
    ".github",
    "/build",
    "docs/devtools/web-ui.md",
    "docs/php/workflows.md"
  ],
  "incremental": false,
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.68.0.wasm",
    "https://plugins.dprint.dev/json-0.15.1.wasm",
    "https://plugins.dprint.dev/markdown-0.13.1.wasm",
    "https://plugins.dprint.dev/prettier-0.7.0.json@4e846f43b32981258cef5095b3d732522947592e090ef52333801f9d6e8adb33"
  ]
}
